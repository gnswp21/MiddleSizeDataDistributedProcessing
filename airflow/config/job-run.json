{
  "name": "dev-job",
  "virtualClusterId": "9npapnc9scs710z1wabnvd6tq",
  "executionRoleArn": "arn:aws:iam::691487686124:role/EMRContainers-JobExecutionRole",
  "releaseLabel": "emr-6.5.0-latest",
  "jobDriver": {
    "sparkSubmitJobDriver": {
      "entryPoint": "local:///etl/src/task.py",
      "entryPointArguments": [
        "dev-task"
      ],
      "sparkSubmitParameters": "--py-files local:///etl/dependency_packages.zip --conf spark.eventLog.enabled=true --conf spark.eventLog.dir=s3://middle-dataset-logging/spark-logs/ --conf spark.history.fs.logDirectory=s3://middle-dataset-logging/spark-logs/"

    }
  },

  "configurationOverrides": {
    "applicationConfiguration": [
      {
        "classification": "spark-defaults",
        "properties": {
          "spark.kubernetes.container.image": "691487686124.dkr.ecr.ap-northeast-2.amazonaws.com/emr6.5_mid_repo"
        }
      }
    ],
    "monitoringConfiguration": {
      "cloudWatchMonitoringConfiguration": {
        "logGroupName": "middle-dataset-etl-log-group",
        "logStreamNamePrefix": "middle-dataset-etl-log-group-prefix"
      },
      "s3MonitoringConfiguration": {
        "logUri": "s3://middle-dataset-logging"
      }
    }
  }
}